package com.WrapperClass;

public class WrapperClassDemo {

	public static void main(String args[]) {
		int i =10; //primitive data types
		float f;
		
		Integer i2 = Integer.valueOf(i);//Boxing,wrapping
		Float floatwrapper = Float.valueOf(10.5f);
		int floatToInt = floatwrapper.intValue();
		
		int j = i2.intValue(); //unboxing
		
		Integer i3 =20; //Integer.valueOf(20) -autoboxing
		
		int k =i2; //Autoboxing
		
		String s="123";
		
		int i4=Integer.parseInt(s);
		System.out.println("Float To Int Value : " +floatToInt);
		System.out.println("i Value : " +i);
		System.out.println("i2 Value : " +i2);
		System.out.println("j Value : " +j);
		System.out.println("i3 Value : " +i3);
		System.out.println("k Value : " +k);
		System.out.println("i4 Value : " +i4);
		
		

	}

}
